{
    "name": "rezaghz/laravel-reports",
    "description": "Laravel reports package for implementing reports (eg: Spam,Violence,Child Abuse,illegal Drugs etc) on Eloquent models.",
    "type": "library",
    "keywords": [
        "laravel",
        "eloquent",
        "report",
        "like",
        "dislike",
        "unlike",
        "rate",
        "rating",
        "emotion",
        "star"
    ],
    "homepage": "https://rezaghz.com",
    "license": "MIT",
    "authors": [
        {
            "name": "reza ghasemzadeh",
            "email": "rezaghasemzadeh1@gmail.com",
            "homepage": "https://rezaghz.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": "^6.20.27|^7.0|^8.0|^9.0|^10.0",
        "illuminate/events": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^7.5|^8.0|^9.0",
        "vimeo/psalm": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Rezaghz\\Laravel\\Reports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rezaghz\\Tests\\Laravel\\Reports\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rezaghz\\Laravel\\Reports\\ReportsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "funding": [
        {
            "type": "other",
            "url": "https://yekpay.me/en/rezaghz"
        }
    ]
}
